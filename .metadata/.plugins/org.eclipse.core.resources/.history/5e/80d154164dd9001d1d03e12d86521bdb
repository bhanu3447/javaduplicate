package two_dimesionl_arrays;

import java.util.Scanner;

public class Declaring2Darray
{

@SuppressWarnings("resource")
public static void main(String[] args) 
 {
	int[][]a= {{2,5,6,8},{4,8,0,77,9},{8,72,92,7}};
	for(int[]s:a)
	{
		for(int z:s)
		{
			System.out.println(z);
		}
	}
	
 }
	
	
 }
