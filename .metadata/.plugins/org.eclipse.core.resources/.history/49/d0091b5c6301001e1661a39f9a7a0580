package javaPractice;

public class Sorting 
{
  public static void main(String[] args) 
  {
	int a[]= {12,2,4,6,9};
	bubbleSort(a);
	for(int c:a)
	{
		System.out.println(c);
	}
  }
  public static void bubbleSort(int a[])
  {
	  for(int i=0;i<a.length-1;i++)
	  {
		  for(int j=0;j<a.length-1-i;j++)
		  {
			  if(a[j]>a[j+1])
			  {
				  int temp=a[j];
				  a[j]=a[j+1];
				  a[j+1]=temp;
			  }
		  }
	  }
  }
}
